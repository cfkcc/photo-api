<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="PayChannelMapper">

	<select id="findById" resultType="com.photo.api.model.pay.PayChannel">
		select * from pay_channel where channel_id = #{channelId}
	</select>
	<select id="findByName" resultType="com.photo.api.model.pay.PayChannel">
		select * from pay_channel where channel_name = #{channelName}
	</select>
	<select id="findAll" resultType="com.photo.api.model.pay.PayChannel">
		select * from pay_channel
	</select>
</mapper>